  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-stockage":
      "node ./index-stockage.js && npx jsonlint exports/titres-stockage.json > exports/titres-stockage-format.json",
    "watch-stockage":
      "onchange -i ./index-stockage.js -- npm run build-stockage",
    "build-hydrocarbures":
      "node ./index-hydrocarbures.js && npx jsonlint exports/titres-hydrocarbures.json > exports/titres-hydrocarbures-format.json",
    "watch-hydrocarbures":
      "onchange -i ./index-hydrocarbures.js -- npm run build-hydrocarbures",
    "build-mineraux":
      "node ./index-mineraux.js && npx jsonlint exports/titres-mineraux.json > exports/titres-mineraux-format.json",
    "watch-mineraux":
      "onchange -i ./index-mineraux.js -- npm run build-mineraux",
